{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "extends": ["./base.json"],
  "plugins": [
    "unicorn",
    "typescript",
    "oxc",
    "import",
    "promise",
    "jsdoc",
    "react",
    "react-perf",
    "jsx-a11y"
  ],
  "rules": {
    "react/jsx-max-depth": "off",
    "react/react-in-jsx-scope": "off"
  },
  "settings": {
    "jsx-a11y": {
      "polymorphicPropName": "render",
      "components": {},
      "attributes": {}
    },
    "react": {
      "formComponents": [],
      "linkComponents": [],
      "version": null
    }
  }
}
