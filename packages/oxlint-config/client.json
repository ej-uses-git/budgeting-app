{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "extends": ["./base.json"],
  "plugins": [
    "unicorn",
    "typescript",
    "oxc",
    "import",
    "vitest",
    "promise",
    "jsdoc",
    "react",
    "react-perf",
    "jsx-a11y"
  ],
  "settings": {
    "jsx-a11y": {
      "polymorphicPropName": "render",
      "components": {},
      "attributes": {}
    },
    "react": {
      "formComponents": [],
      "linkComponents": [],
      "version": null
    }
  }
}
